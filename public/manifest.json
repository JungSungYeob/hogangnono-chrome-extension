{
    "name": "React Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": [
          "contentScript.js"
        ]
      }
    ],
    "options_page": "options.html",
    "permissions": [
      "tabs",
      "bookmarks"
    ]
  }